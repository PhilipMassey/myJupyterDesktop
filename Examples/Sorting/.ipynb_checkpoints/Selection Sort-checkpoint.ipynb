   "source": [
    "import random\n",
    "n = 20\n",
    "arr = random.sample(range(n),n)\n",
    "arr\n",
    "for i in range(n):\n",
    "    minimum = i\n",
    "    for j in range(i+1,n):\n",
    "        if arr[j] < arr[minimum]:\n",
    "            minimum = j\n",
    "    if minimum != i:\n",
    "        arr[i],arr[minimum] = arr[minimum], arr[i]\n",
    "arr"
   ]
  }
